{
  "manifest_version": 3,
  "name": "Locator Inspector",
  "version": "0.1",
  "icons": {
    "16": "logo/16.png",
    "48": "logo/48.png",
    "128": "logo/128.png"
  },
  "permissions": ["scripting", "activeTab", "storage", "sidePanel"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Open Locator Inspector",
    "default_icon": {
      "16": "logo/16.png",
      "48": "logo/48.png"
    }
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
